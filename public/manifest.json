{
  "manifest_version": 3,
  "name": "Uta Cookies",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "cookies",
    "downloads",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  }
}